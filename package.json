{
  "name": "@sepiariver/binliner-js",
  "version": "2.0.1",
  "description": "Binary sequence validator for JavaScript. **Breaking changes in >= 2.0.0**",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "browser": "./dist/index.js",
  "exports": {
    ".": {
      "bun": "./src/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js",
      "browser": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "bun run build:esm && bun run build:cjs",
    "build:esm": "bun build src/index.js --outdir ./dist --target browser --format esm",
    "build:cjs": "bun build src/index.js --outdir ./dist --target node --format cjs --outfile dist/index.cjs.js",
    "test": "bun test",
    "preflight": "bun run build && bun run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sepiariver/binliner-js.git"
  },
  "keywords": [
    "binary",
    "sequence",
    "tree",
    "heap",
    "validator",
    "conditional logic"
  ],
  "author": "sepiariver",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sepiariver/binliner-js/issues"
  },
  "homepage": "https://github.com/sepiariver/binliner-js#readme"
}
